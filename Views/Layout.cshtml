@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@using System.Globalization
@using System.Web.Optimization;
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <title>@Model.Value("pageTitle")</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="@Model.Value("pageDescription")" />
    <meta name="keywords" content="@Model.Value("pageKeywords")" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="@Url.Content("~/assets/css/style.css")">

</head>
<body class="landAcquisition">
@Styles.Render("~/bundles/styles/global.css")
    @Styles.Render("~/css/Uberflip/style.css")
    @*@Styles.Render("~/css/Footer.css")*@
    @Styles.Render("~/css/Solution.css")
@Umbraco.RenderMacro("MainNavigation")

    @RenderBody()
@Umbraco.RenderMacro("MainFooter")
            @Scripts.Render("~/bundles/scripts/global.js")
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>